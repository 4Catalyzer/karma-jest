{
  "name": "karma-jest",
  "version": "1.0.0-beta.8",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/4Catalyzer/karma-jest.git"
  },
  "author": "4Catalyzer",
  "license": "MIT",
  "scripts": {
    "prebuild": "patch-package",
    "build": "rollup -c rollup.config.js && cp -r src/assets lib/assets",
    "format": "4c format src './*'",
    "lint": "4c lint src './*'",
    "prepublishOnly": "yarn run build",
    "release": "4c release",
    "test": "yarn lint && yarn typecheck",
    "typecheck": "tsc --noEmit",
    "postinstall": "patch-package"
  },
  "files": [
    "lib"
  ],
  "publishConfig": {
    "access": "public",
    "directory": "lib"
  },
  "prettier": "@4c/prettier-config",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*": "yarn 4c lint --fix"
  },
  "peerDependencies": {
    "karma": "*"
  },
  "devDependencies": {
    "@4c/babel-preset": "^7.3.6",
    "@4c/cli": "^2.1.10",
    "@4c/prettier-config": "^1.1.0",
