{
  "name": "karma-jest",
  "version": "1.0.0",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/4Catalyzer/karma-jest.git"
  },
  "author": "4Catalyzer",
  "license": "MIT",
  "scripts": {
    "prebuild": "patch-package",
    "build": "rollup -c rollup.config.js",
    "format": "4c format src './*'",
    "lint": "4c lint src './*'",
    "prepublishOnly": "yarn run build",
    "release": "4c release",
    "tdd": "jest --watch",
    "test": "yarn lint && jest && yarn typecheck",
    "testonly": "jest",
    "typecheck": "tsc --noEmit",
    "postinstall": "patch-package"
  },
  "publishConfig": {
    "access": "public"
  },
  "prettier": "@4c/prettier-config",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*": "yarn 4c lint --fix"
  },
  "jest": {
    "preset": "@4c/jest-preset",
    "testEnvironment": "node"
  },
  "devDependencies": {
    "@4c/babel-preset": "^7.3.3",
    "@4c/cli": "^2.0.1",
    "@4c/jest-preset": "^1.5.0",
    "@4c/prettier-config": "^1.1.0",
    "@4c/tsconfig": "^0.3.1",
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.7",
    "@babel/preset-typescript": "^7.8.3",
    "@babel/register": "^7.8.6",
    "@rollup/plugin-replace": "^2.3.1",
    "@types/karma": "^4.4.0",
    "@types/node": "*",
    "@typescript-eslint/eslint-plugin": "^2.22.0",
    "@typescript-eslint/parser": "^2.22.0",
    "browser-jest-message-util": "npm:jest-message-util@25.1.0",
    "browser-jest-snapshot": "npm:jest-snapshot@25.1.0",
    "browser-jest-util": "npm:jest-util@25.1.0",
    "eslint": "^6.8.0",
    "eslint-config-4catalyzer": "^1.1.1",
    "eslint-config-4catalyzer-jest": "^2.0.5",
    "eslint-config-4catalyzer-typescript": "^1.1.7",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "karma": "^4.4.1",
    "karma-mocha-reporter": "^2.2.5",
    "karma-rollup-preprocessor": "^7.0.3",
    "lint-staged": "^10.0.8",
    "patch-package": "^6.2.1",
    "prettier": "^1.19.1",
    "rollup": "^2.0.2",
    "rollup-plugin-alias": "^2.2.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-cjs-es": "^0.9.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-visualizer": "^3.3.1",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@4c/cli-core": "^2.1.1",
    "@babel/code-frame": "^7.8.3",
    "@jest/console": "^25.1.0",
    "@jest/reporters": "^25.1.0",
    "@jest/test-result": "^25.1.0",
    "@jest/types": "^25.1.0",
    "@rollup/plugin-alias": "^3.0.1",
    "@types/babel__code-frame": "^7.0.1",
    "@types/marked": "^0.7.2",
    "@types/mocha": "^7.0.1",
    "ansi-colors": "^4.1.1",
    "error-stack-parser": "^2.0.6",
    "expect": "^25.1.0",
    "jest-circus": "^25.1.0",
    "jest-message-util": "^25.1.0",
    "jest-mock": "^25.1.0",
    "jest-snapshot": "25.1.0",
    "marked": "^0.8.0",
    "source-map": "^0.7.3"
  }
}
