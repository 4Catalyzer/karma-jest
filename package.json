    "@babel/core": "^7.10.3",
    "@babel/preset-typescript": "^7.10.1",
    "@babel/register": "^7.10.3",
    "@rollup/plugin-replace": "^2.3.3",
    "@types/karma": "^5.0.0",
    "@types/node": "*",
    "@typescript-eslint/eslint-plugin": "^3.3.0",
    "@typescript-eslint/parser": "^3.3.0",
    "browser-jest-message-util": "npm:jest-message-util@26.0.1",
    "browser-jest-snapshot": "npm:jest-snapshot@26.0.1",
    "browser-jest-util": "npm:jest-util@26.0.1",
    "eslint-config-4catalyzer": "^1.1.2",
    "eslint-config-4catalyzer-jest": "^2.0.7",
    "eslint-config-4catalyzer-typescript": "^2.0.2",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jest": "^23.16.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "karma": "^5.1.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-firefox-launcher": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-rollup-preprocessor": "^7.0.5",
    "lint-staged": "^10.2.11",
    "patch-package": "^6.2.2",
    "prettier": "^2.0.5",
    "rollup": "^2.17.1",
    "rollup-plugin-alias": "^2.2.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-cjs-es": "^1.0.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-visualizer": "^4.0.4",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "@4c/cli-core": "^2.1.5",
    "@babel/code-frame": "^7.8.3",
    "@jest/console": "^26.0.0",
    "@jest/fake-timers": "^26.0.0",
    "@jest/reporters": "^26.0.0",
